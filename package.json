{
  "name": "backend-2",
  "version": "1.0.0",
  "description": "Steps to run this project:",
  "main": "index.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "dev": "npm run swagger && concurrently \"nodemon\" \"ts-node-dev --respawn startServer.ts\"",
    "dev-i": "npm run swagger && concurrently \"nodemon\" \"ts-node-dev --inspect --respawn startServer.ts\"",
    "ts-node-dev": "ts-node-dev",
    "build": "tsc --outDir build --experimentalDecorators",
    "prod": "node build/startServer.js",
    "test": "mocha -r ts-node/register tests/**/*.test.ts --exit",
    "start": "ts-node src/index.ts",
    "routes": "tsoa routes",
    "swagger": "tsoa spec",
    "dev-cert": "node cert/generateCerts.js",
    "prod-cert": "node build/cert/generateCerts.js",
    "typeorm": "node ./node_modules/typeorm/cli.js",
    "migrate": "sequelize db:migrate",
    "migrate-testing": "NODE_ENV=test sequelize db:migrate",
    "undo-migrate": "sequelize db:migrate:undo",
    "undo-migrate-all": "sequelize db:migrate:undo:all",
    "undo-migrate-specific": "sequelize db:migrate:undo --name",
    "create-migration": "sequelize migration:generate --name",
    "create-seed": "sequelize seed:generate --name",
    "seeds": "sequelize db:seed:all",
    "seeds-testing": "NODE_ENV=test sequelize db:seed:all",
    "unseed": "sequelize db:seed:undo:all"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.20.2",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.1",
    "jwt-decode": "^3.1.2",
    "moment": "^2.29.4",
    "node-forge": "^1.3.1",
    "pg": "^8.11.2",
    "pg-hstore": "^2.3.4",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^6.32.1",
    "sequelize-cli": "^6.6.1",
    "sequelize-typescript": "^2.1.5",
    "sqlite3": "^5.1.6",
    "swagger-ui-express": "^5.0.0",
    "tsoa": "^5.1.1",
    "uuid": "^9.0.0",
    "winston": "^3.10.0"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.2",
    "@types/chai": "^4.3.5",
    "@types/express": "^4.17.17",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/mocha": "^10.0.1",
    "@types/node": "^20.4.9",
    "@types/node-forge": "^1.3.4",
    "@types/sequelize": "^4.28.15",
    "@types/supertest": "^2.0.12",
    "@types/swagger-ui-express": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "^6.3.0",
    "@typescript-eslint/parser": "^6.3.0",
    "chai": "^4.3.7",
    "concurrently": "^8.2.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.28.0",
    "eslint-plugin-prettier": "^5.0.0",
    "mocha": "^10.2.0",
    "nodemon": "^3.0.1",
    "should": "^13.2.3",
    "supertest": "^6.3.3",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.1.6"
  }
}
