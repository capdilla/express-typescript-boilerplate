{
  "name": "backend-typescript",
  "version": "1.0.0",
  "description": "",
  "main": "startServer.ts",
  "scripts": {
    "dev": "npm run swagger && concurrently \"nodemon\" \"ts-node-dev --respawn startServer.ts\"",
    "dev-i": "npm run swagger && concurrently \"nodemon\" \"ts-node-dev --inspect --respawn startServer.ts\"",
    "build": "tsc --outDir build --experimentalDecorators",
    "prod": "node build/startServer.js",
    "test": "mocha -r ts-node/register tests/**/*.test.ts --exit",
    "start": "ts-node src/index.ts",
    "routes": "tsoa routes",
    "swagger": "tsoa spec",
    "dev-cert": "node cert/generateCerts.js",
    "prod-cert": "node build/cert/generateCerts.js",
    "typeorm": "node ./node_modules/typeorm/cli.js",
    "migrate": "sequelize db:migrate",
    "migrate-testing": "NODE_ENV=test sequelize db:migrate",
    "undo-migrate": "sequelize db:migrate:undo",
    "undo-migrate-all": "sequelize db:migrate:undo:all",
    "undo-migrate-specific": "sequelize db:migrate:undo --name",
    "create-migration": "sequelize migration:generate --name",
    "create-seed": "sequelize seed:generate --name",
    "seeds": "sequelize db:seed:all",
    "seeds-testing": "NODE_ENV=test sequelize db:seed:all",
    "unseed": "sequelize db:seed:undo:all"
  },
  "repository": {
    "type": "git",
    "url": "git+https://bitbucket.org/diatomea/backend.git"
  },
  "author": "",
  "license": "ISC",
  "homepage": "https://bitbucket.org/diatomea/backend#readme",
  "dependencies": {
    "body-parser": "^1.19.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "jwt-decode": "^3.1.2",
    "moment": "^2.29.1",
    "node-forge": "^0.10.0",
    "pg": "^8.5.1",
    "pg-hstore": "^2.3.3",
    "reflect-metadata": "^0.1.10",
    "sequelize": "~5.21.2",
    "sequelize-cli": "^6.2.0",
    "sequelize-typescript": "~1.0.0",
    "sqlite3": "^5.0.0",
    "swagger-ui-express": "^4.1.4",
    "tsoa": "^3.4.0",
    "uuid": "^8.3.1",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/express": "^4.17.9",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/node-forge": "^0.9.5",
    "@types/sequelize": "^4.28.9",
    "@types/body-parser": "^1.19.0",
    "@types/chai": "^4.2.14",
    "@types/mocha": "^8.0.4",
    "@types/node": "^8.0.29",
    "@types/supertest": "^2.0.10",
    "@types/swagger-ui-express": "^4.1.2",
    "chai": "^4.2.0",
    "concurrently": "^5.3.0",
    "mocha": "^8.2.1",
    "nodemon": "^2.0.6",
    "should": "^13.2.3",
    "supertest": "^6.0.1",
    "ts-node": "3.3.0",
    "ts-node-dev": "^1.0.0",
    "typescript": "3.9.2"
  }
}
